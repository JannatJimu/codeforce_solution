import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner in = new Scanner (System.in);
       long n,m,a;
      n=in.nextLong();
      m=in.nextLong();
      a=in.nextLong();
      long x= n/a;
      long y=m/a;
      {
      if(n%a!=0)
      {
          x++;
      }
      }
      if(m%a!=0)
      {
          y++;
      }
        System.out.println("" +x*y);
     }
    
}
